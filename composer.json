{
    "name": "sylapi/eurocommerce-linker",
    "type": "library",
    "description": "Eurocommerce Linker",
    "keywords": [
        "package"
    ],
    "license": "MIT",
    "authors": [
    ],
    "require": {
        "php": ">=8.0.0",
        "rakit/validation": "^1.4",
        "guzzlehttp/guzzle": "^7.3"
    },
    "repositories": [
    ],    
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan": "^0.12.77"
    },
    "autoload": {
        "psr-4": { "Sylapi\\EurocommerceLinker\\" : "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Sylapi\\EurocommerceLinker\\Tests\\": "tests/" }
    },
    "scripts": {
        "tests": "vendor/bin/phpunit",
        "phpstan": "vendor/bin/phpstan analyse src tests",
        "coverage": "XDEBUG_MODE=coverage  vendor/bin/phpunit --verbose --coverage-text",
        "coverage-html": "XDEBUG_MODE=coverage  vendor/bin/phpunit --coverage-html ./coverage/"
    },    
    "prefer-stable": false
}
